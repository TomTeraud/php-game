{
    "name": "tomst/phppractice",
    "description": "Prototype app for learning Docker and PHP",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
      {
        "name": "Tom Terauds",
        "email": "tomsterauds@gmail.com"
      }
    ],
    "require": {
      "php": "^8.4",
      "ext-pdo": "*",
      "ext-pdo_mysql": "*",
      "symfony/cache": "^7.2",
      "predis/predis": "^2.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^12.0"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "scripts": {
      "run-phpunit": "vendor/bin/phpunit"
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist"
    },
    "support": {
      "issues": "https://github.com/tomst/phppractice/issues",
      "source": "https://github.com/tomst/phppractice"
    }
  }
  